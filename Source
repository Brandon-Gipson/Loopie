#include <iostream>

using namespace std;

void main()
{
	cout << "If nothing below this line, exit program." << endl;
	double		dX;
	double		dY;
	double		dZ;
	float		fX;
	float		fY;

	dX = 1;
	dY = 0;
	for (dX = 1; dX <= 1000000000; dX++);
	{
		dZ = dY + (1 / dX);
		cout.precision(7);
		cout << "dZ is " << fX << endl;
			
	}

}
