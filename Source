// Loops

#include <iostream>

using namespace std;

void main()
{
	cout << "If nothing below this line, exit program." << endl;
	double		dX;
	double		dY;
	double		dZ;
	float		fX;
	float		fY;
	float		fZ;

	dX = 1;
	dY = 0;
	dZ = 0;
	for (dX = 1; dX <= 100000000; dX++);
	{
		fX = dZ + (1 / dX);
		cout.precision(14);
		cout << "dZ is " << fX << endl;

	}

}
