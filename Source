#include <iostream>

using namespace std;

int main()
{
	cout << "If nothing below this line, exit program." << endl;
	double		dX;
	double		dY;
	double		dZ;
	float		fX;
	float		fY;
	float		fZ;

	dX = 1;
	dY = 0;
	for (dX = 1; dX <= 5; dX++)
	{
		dZ += dX;
	}
	
	cout << "dZ is " << dZ << endl;
	
	return 0;
}
